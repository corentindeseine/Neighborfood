{
  "background_color": "#e6e7ee",
  "description": "Neighborfood Réservez vos plats du soir à l'un des nombreux chefs cuistots de votre quartier ! \n Avec neighborfood, rencontrez vos voisins et découvrez de nouvelles saveures à des prix raisonnables",
  "display": "fullscreen",
  "name": "Neighborfood",
  "orientation": "portrait",
  "icons": [
    <% files = Dir.entries(Rails.root.join("app/assets/images/icons/")).select {|f| !File.directory? f} %>
    <% files.each_with_index do |file, index| %>
      <% match = file.match(/.+-(?<size>\d{2,3}x\d{2,3}).png/) %>
      {
        "src": "<%= image_path "icons/#{file}" %>",
        "sizes": "<%= match && match[:size] %>",
        "type": "image/png"
      }<%= "," unless (files.size - 1) == index %>
    <% end %>
  ]
}
